{
  "name": "fairhold-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "backup": "docker-compose --profile backup --env-file .env.production up",
    "build": "npm run prisma:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest; open ./coverage/lcov-report/index.html",
    "prisma:pull": "node load-env-and-prisma.js db pull",
    "prisma:generate": "prisma generate",
    "postinstall": "npm run prisma:generate",
    "prettier": "prettier --write .",
    "prepare": "husky",
    "docs": "typedoc && npm run map",
    "map": "tsuml2 --glob app/models/**/*.ts --modifiers false --propertyTypes false -m -o docs/class-map.svg"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@prisma/client": "^5.20.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "chart.js": "^4.4.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "lucide-react": "^0.453.0",
    "mathjs": "^13.1.1",
    "next": "^14.2.15",
    "postcode": "^5.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.1",
    "react-spinners": "^0.14.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.7",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "3.3.3",
    "prisma": "^5.21.1",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.2.5",
    "tsuml2": "^0.17.0",
    "typedoc": "^0.26.10",
    "typescript": "^5"
  }
}
